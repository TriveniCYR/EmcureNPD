                                                                    <div class="listCcheckbox">
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.Quest1Navigation.SoluInfusion, true, new { @class = "spouseClass", disabled = "disabled" })
                                                                                <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                                                                @SharedLocalizer["Yes"].Value
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td style="min-width: 90px;">
                                                                    <div class="listCcheckbox">
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.Quest1Navigation.SoluInfusion, false, new { @class = "spouseClass", disabled = "disabled" })
                                                                                <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                                                                @SharedLocalizer["No"].Value
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </tr>

                                                            <tr>
                                                                <td>2.</td>
