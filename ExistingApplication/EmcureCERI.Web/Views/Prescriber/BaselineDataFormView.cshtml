@inject Microsoft.Extensions.Localization.IStringLocalizer<SharedResource> SharedLocalizer
@model EmcureCERI.Data.DataAccess.Entities.BaselineDataMaster
@{
    ViewData["Title"] = @SharedLocalizer["Baseline Form View"].Value;
}
<style type="text/css">
    .field-validation-error {
        color: #ff0000;
        display: block;
    }
</style>
<section class="section">
    <div class="container-fluid">
        <h4>@Model.Patient.FirstName @Model.Patient.LastName</h4>
        <div class="tabsContainer">
            <ul class="nav nav-pills toptabs">
                <li><a asp-area="" asp-controller="Prescriber" asp-action="InformedConsentForm" asp-route-id="@ViewBag.patientid">@SharedLocalizer["Informed Consent Form"].Value</a></li>
                <li class="active"><a asp-area="" asp-controller="Prescriber" asp-action="BaselineDataForm" asp-route-id="@ViewBag.patientid">@SharedLocalizer["Baseline Data Form"].Value</a></li>
                <li><a asp-area="" asp-controller="Prescriber" asp-action="FollowUpForm" asp-route-id="@ViewBag.patientid">@SharedLocalizer["Follow Up Form"].Value</a></li>
            </ul>
            <div class="tab-content clearfix">
                <div class="leftTabButtonContainer">
                    <ul class="nav nav-pills nav-stacked" id="myTab">
                        <li class="active"><a href="#eligibilityData" data-toggle="tab"><div id="tab1Id" class="@(Model.Quest1Navigation.IsFulFill == true ? "checkboxActiveDummy" : "checkboxEmptyDummy")"></div>@SharedLocalizer["Informed Consent and Eligibility"].Value.ToUpper()</a></li>
                        <li><a href="#demographicData" data-toggle="tab"><div id="tab2Id" class="@(Model.Quest2Navigation.IsFulFill == true ? "checkboxActiveDummy" : "checkboxEmptyDummy")"></div>@SharedLocalizer["Demographic Data"].Value.ToUpper()</a></li>
                        <li><a href="#treatmentData" data-toggle="tab"><div id="tab3Id" class="@(Model.Quest3Navigation.IsFulFill == true ? "checkboxActiveDummy" : "checkboxEmptyDummy")"></div>@SharedLocalizer["Treatment Init Details"].Value.ToUpper()</a></li>
                        <li><a href="#presciberSignOffData" data-toggle="tab"><div id="tab4Id" class="@(Model.Quest4Navigation.IsFulFill == true ? "checkboxActiveDummy" : "checkboxEmptyDummy")"></div>@SharedLocalizer["Prescriber’s Sign Off"].Value.ToUpper()</a></li>
                    </ul>
                    <ul hidden>
                        <li>@Html.CheckBoxFor(x => x.Quest1Navigation.IsFulFill)</li>
                        <li>@Html.CheckBoxFor(x => x.Quest2Navigation.IsFulFill)</li>
                        <li>@Html.CheckBoxFor(x => x.Quest3Navigation.IsFulFill)</li>
                        <li>@Html.CheckBoxFor(x => x.Quest4Navigation.IsFulFill)</li>
                    </ul>
                </div>
                <div class="rightTabContainer">
                    <div class="tab-content">
                        <div class="tab-pane active" id="eligibilityData">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="panel-group custInFormPanel" id="accordion">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">
                                                    <a data-toggle="collapse" class="collapsed" data-parent="#accordion" href="#collapse0">@SharedLocalizer["Informed Consent"].Value.ToUpper()</a>
                                                </h4>
                                            </div>
                                            <div id="collapse0" class="panel-collapse collapse in" aria-expanded="true">
                                                <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label>@SharedLocalizer["Informed Consent Given"].Value</label>
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.Quest1Navigation.Icg, true, new { @class = "spouseClass", disabled = "disabled" })
                                                                                <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                                                                Yes
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-6">
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.Quest1Navigation.Icg, false, new { @class = "spouseClass", disabled = "disabled" })
                                                                                <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                                                                No
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <label>@SharedLocalizer["Date participant/patient proxy signed written consent form"].Value</label>
                                                            <div class="dummyInputBox">@(Model.Quest1Navigation.DateParticipant.HasValue ? Model.Quest1Navigation.DateParticipant.Value.ToShortDateString() : string.Empty)</div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label>@SharedLocalizer["Country"].Value</label>
