        function SubmitFormConfirm(form) {
            $.validator.unobtrusive.parse(form);
            if ($(form).valid()) {
                $.ajax({
                    type: "POST",
                    url: form.action,
                    data: $(form).serialize(),
                    success: function (data) {
                        if (data.result === "success") {
                            openCommonModal('successModal modal-sm', '@Html.Raw(SharedLocalizer["Success"].Value)', '@Html.Raw(SharedLocalizer["Record has been added successfully."].Value)', false);
                        } else {
                            if (data.result === "error") {
                                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', data.message, false);
                            }
                        }
                    }
                });
            }
            return false;
        }

        function SubmitFormModification(form) {
            $('#loaderContainer').show();
            $.validator.unobtrusive.parse(form);
            if ($(form).valid()) {
                $.ajax({
                    type: "POST",
                    url: form.action,
                    data: $(form).serialize(),
                    success: function (data) {
                        if (data.result === "success") {
                            //openCommonModal('successModal modal-sm', "Success", 'Record has been modified successfully.', false);
                            location.reload(); // reload the page
                            $('#loaderContainer').hide();
                        } else {
                            if (data.result === "error") {
                                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', data.message, false);
                            }
                        }
                    }
                });
            }
            return false;
        }

