            else {
                $("#mediacalHistoryContainer .meiacalHistoryBox").each(function (index) {
                    if ($(this).find('input[type="text"]').val() != "") {
                        $('#mediacalHistoryContainer .meiacalHistoryBox').eq(index).show();
                        $('#mediacalHistoryContainer').attr('row-count', index);
