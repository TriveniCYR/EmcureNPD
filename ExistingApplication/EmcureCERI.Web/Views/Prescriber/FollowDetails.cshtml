                                    <button type="submit" class="btn btn-default btn-sm">@SharedLocalizer["Save"].Value</button>
                                </div>
                            }
                        </div>
                    </div>
                    <br />
                    <br />
                    <br />
                </div>
            </div>
        </div>
    </div>

</section>

@section Scripts {
    <script type="text/javascript">

        function SubmitForm(form) {
            $.validator.unobtrusive.parse(form);
            if ($(form).valid()) {
                $.ajax({
                    type: "POST",
                    url: form.action,
                    data: $(form).serialize(),
                    success: function (data) {
                        if (data.result === "success") {
                            openCommonModal('successModal modal-sm', '@Html.Raw(SharedLocalizer["Success"].Value)','@Html.Raw(SharedLocalizer["Record has been added successfully."].Value)', false);
                            $("#" + data.message + "").addClass("checkboxActiveDummy");
                            switch (data.message) {
                                case "tab1Id":
                                    $('#Fufquest1Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab2Id":
                                    $('#Fufquest2Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab3Id":
                                    $('#Fufquest3Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab4Id":
                                    $('#Fufquest4Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab5Id":
                                    $('#Fufquest5Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab6Id":
                                    $('#Fufquest6Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab7Id":
                                    $('#Fufquest7Navigation_IsFulFill').attr('checked', true);
                                    break;
                            }
                        } else {
                            if (data.result === "error") {
                                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', data.message, false);
                            }
                        }
                    }
                });
            } else {
                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', '@SharedLocalizer["Please fill required fields, in every section."].Value', false);
            }
            return false;
        }


        //For Adverse Events Add Remove
        $('#addAdverseEvents').on('click', function () {
            var mediBoxNo = parseInt($('#adverseEventContainer').attr('row-count'));
            addAdverseEvents(mediBoxNo);
        })

        addAdverseEvents = function (mediBoxNo) {
            $('#adverseEventContainer').attr('row-count', mediBoxNo + 1);
            $('#adverseEventContainer .meiacalHistoryBox').eq($('#adverseEventContainer').attr('row-count')).show();
        }

        $('#removeAdverseEvents').on('click', function () {
            var mediBoxNo = parseInt($('#adverseEventContainer').attr('row-count'));
            $('#adverseEventContainer .meiacalHistoryBox').eq($('#adverseEventContainer').attr('row-count')).hide();
            $('#adverseEventContainer').attr('row-count', mediBoxNo - 1);
        });



        //For Medical History Add Remove
        addMediaclHistory = function (mediBoxNo) {
            $('#mediacalHistoryContainer').attr('row-count', mediBoxNo + 1);
            $('#mediacalHistoryContainer .meiacalHistoryBox').eq($('#mediacalHistoryContainer').attr('row-count')).show();
        }

        $('#addMediaclHistory').on('click', function () {
            var mediBoxNo = parseInt($('#mediacalHistoryContainer').attr('row-count'));
            addMediaclHistory(mediBoxNo);
        })

        $('#removeLastMediaclHistory').on('click', function () {
            var mediBoxNo = parseInt($('#mediacalHistoryContainer').attr('row-count'));
            $('#mediacalHistoryContainer .meiacalHistoryBox').eq($('#mediacalHistoryContainer').attr('row-count')).hide();
            $('#mediacalHistoryContainer').attr('row-count', mediBoxNo - 1);
        });


        //For Medication Add Remove
        addMedication = function (mediBoxNo) {
            $('#medicationsScreeningContainer').attr('row-count', mediBoxNo + 1);
            $('#medicationsScreeningContainer .meiacalHistoryBox').eq($('#medicationsScreeningContainer').attr('row-count')).show();
        }

        $('#addMedication').on('click', function () {
            var mediBoxNo = parseInt($('#medicationsScreeningContainer').attr('row-count'));
            addMedication(mediBoxNo);
        })

        $('#removeLastMedication').on('click', function () {
            var mediBoxNo = parseInt($('#medicationsScreeningContainer').attr('row-count'));
            $('#medicationsScreeningContainer .meiacalHistoryBox').eq($('#medicationsScreeningContainer').attr('row-count')).hide();
            $('#medicationsScreeningContainer').attr('row-count', mediBoxNo - 1);
        })


        $("input[name='Fufquest1Navigation.Pexperienced']").click(function () {
            if ($(this).val() == 'True') {
                $('#adverseEventContainer').show();
            } else {
                $('#adverseEventContainer').hide();
            }
        });

        $("input[name='Fufquest5Navigation.MedicalHistory']").click(function () {
            if ($(this).val() == 'True') {
                $('#mediacalHistoryContainer').show();
            } else {
                $('#mediacalHistoryContainer').hide();
            }
        });

        $("input[name='Fufquest6Navigation.Medications']").click(function () {
            if ($(this).val() == 'True') {
                $('#medicationsScreeningContainer').show();
            } else {
                $('#medicationsScreeningContainer').hide();
            }
        });



        $(function () {
            $('.date').datetimepicker({
