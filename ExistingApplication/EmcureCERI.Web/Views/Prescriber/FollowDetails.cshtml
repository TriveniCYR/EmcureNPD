                $('#medicationsScreeningContainer input[type="text"]').val('');
                $('#medicationsScreeningContainer input[type="checkbox"]').prop("checked", false);
                $("#medicationsScreeningContainer .row:nth-child(4) .col-sm-4:nth-child(2)").show();
            }
            else {

                $("#medicationsScreeningContainer .meiacalHistoryBox").each(function (index) {
                    if ($(this).find('input[type="text"]').val() != "") {
                        $('#medicationsScreeningContainer .meiacalHistoryBox').eq(index).show();
                        $('#medicationsScreeningContainer').attr('row-count', index);

                        if ($(this).find('input[type="checkbox"]').prop("checked") == true) {
                            $(this).find('.row:nth-child(4) .col-sm-4:nth-child(2)').hide();
