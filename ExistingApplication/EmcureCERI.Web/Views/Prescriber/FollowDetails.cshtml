                        $('#specifyreason').show();
                    }
                    else if($(this).prop("checked") == false){
                         $('#specifyreason').hide();
                    }
                
            }); 

            

           if ('@Model.Fufquest3Navigation.Withdrawal' == 'True') {
                $('#specifyreason').show();
            } else {
                $('#specifyreason').hide();
            }

            
            $("input[name='Fufquest3Navigation.OtherOutcomes']").click(function () { 
                if($(this).prop("checked") == true){
                        $('#specify').show();
                    }
                    else if($(this).prop("checked") == false){
                         $('#specify').hide();
                    }
                
            }); 

            if ('@Model.Fufquest3Navigation.OtherOutcomes' == 'True') {
                $('#specify').show();
            } else {
                $('#specify').hide();
            }


            if ('@Model.Fufquest1Navigation.Pexperienced' == 'False') {
                $('#adverseEventContainer').hide();

                $('#adverseEventContainer input[type="text"]').val('');
                $('#adverseEventContainer select').val(null);
            } else {

                $("#adverseEventContainer .meiacalHistoryBox").each(function (index) {
                     if ($(this).find('input[type="text"]').val() != "") {
                         $('#adverseEventContainer .meiacalHistoryBox').eq(index).show();
