          //Date picker validation start date and End Date
          $('.date[data-date="startDate"]').on('dp.change', function (e) {
            $(this).parents('.row').find('.date[data-date="stopDate"]').data('DateTimePicker').minDate(e.date);
          });
          $('.date[data-date="stopDate"]').on('dp.show', function () {
            $(this).data('DateTimePicker').minDate($(this).parents('.row').find('.date[data-date="startDate"]').data('DateTimePicker').date());
          });


        });


        //Has the patient been diagnosed with any malignancy since initiation of Cidofovir injection
        $("input[name='Fufquest1Navigation.CidofovirInjection']").click(function () {
            if ($(this).val() == 'True') {
                $('#specifyId').show();
                $("input[name='Fufquest1Navigation.Specify']").val('');
            } else {
                $('#specifyId').hide();
                $("input[name='Fufquest1Navigation.Specify']").val('');
            }
        });



        // Or tick if Ongoing
        $("#mediacalHistoryContainer .meiacalHistoryBox input[type='checkbox']").click(function () {
            if ($(this).prop("checked") == true) {
                $(this).parents('.row').find('.col-sm-4:nth-child(3)').hide();
                $(this).parents('.row').find('.col-sm-4:nth-child(3) input[type="text"]').val('');
            } else {
                $(this).parents('.row').find('.col-sm-4:nth-child(3)').show();
