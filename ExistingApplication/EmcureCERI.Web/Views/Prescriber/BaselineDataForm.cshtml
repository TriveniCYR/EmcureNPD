@section Scripts {
    <script type="text/javascript">
        function PopupFormPreview(url) {
            var message = "";
            if ($('#Quest4Navigation_IsFulFill').is(":not(:checked")) {
                message = "@SharedLocalizer["Please"].Value @SharedLocalizer["Prescriber’s Sign Off"].Value @SharedLocalizer["Details"].Value \n" + message;
            }
            if ($('#Quest3Navigation_IsFulFill').is(":not(:checked")) {
                message = "@SharedLocalizer["Please"].Value @SharedLocalizer["Treatment Init Details"].Value @SharedLocalizer["Details"].Value \n" + message;
                //message = "Please Treatment Init Details \n" + message;
            }
            if ($('#Quest2Navigation_IsFulFill').is(":not(:checked")) {
                message = "@SharedLocalizer["Please"].Value @SharedLocalizer["Demographic Data"].Value @SharedLocalizer["Details"].Value \n" + message;
                //message = "Please Demographic Data Details \n" + message;
            }
            if ($('#Quest1Navigation_IsFulFill').is(":not(:checked")) {
                message = "@SharedLocalizer["Please"].Value @SharedLocalizer["Informed Consent and Eligibility"].Value @SharedLocalizer["Details"].Value \n" + message;
                //message = "Please Informed Consent and Eligibility Details \n" + message;
            }

            if (message == "") {
                $.post(url)
                    .done(function (data) {
                        if (data.result === "success") {
                            location.reload(); // reload the page
                        } else {
                            if (data.result === "error") {
                                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', data.message, false);
                            }
                        }
                    });
            } else {
                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', message, false);
            }
        }

        function SubmitForm(form) {
            $.validator.unobtrusive.parse(form);
            if ($(form).valid()) {
                $.ajax({
                    type: "POST",
                    url: form.action,
                    data: $(form).serialize(),
                    success: function (data) {
                        if (data.result === "success") {
                            openCommonModal('successModal modal-sm', '@Html.Raw(SharedLocalizer["Success"].Value)', '@Html.Raw(SharedLocalizer["Record has been added successfully."].Value)', false);
                            $("#" + data.message + "").addClass("checkboxActiveDummy");
                            switch (data.message) {
                                case "tab1Id":
                                    $('#Quest1Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab2Id":
                                    $('#Quest2Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab3Id":
                                    $('#Quest3Navigation_IsFulFill').attr('checked', true);
                                    break;
                                case "tab4Id":
                                    $('#Quest4Navigation_IsFulFill').attr('checked', true);
                                    break;
                            }
                        } else {
                            if (data.result === "error") {
                                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', data.message, false);
                            }
                        }
                    }
                });
            } else {
                openCommonModal('alertModal modal-sm', '@SharedLocalizer["Error"].Value', '@SharedLocalizer["Please fill required fields, in every section."].Value', false);
            }
            return false;
        }
