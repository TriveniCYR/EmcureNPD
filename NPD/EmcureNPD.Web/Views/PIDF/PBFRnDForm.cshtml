                </div>
                <div class="col-md-9 pl-lg-5">
                    <ul class="nav nav-pills" id="ProductStrengthTabs">
                        @if (Model.MasterStrengthEntities.Count() > 0 || Model.MasterStrengthEntities != null)
                        {
                            int k = 0;
							@foreach (var _strength in Model.MasterStrengthEntities)
                            {
                                <li class="nav-item mr-2">
									<a class="btn btn-outline-primary" href="#tab_@_strength.Strength" onClick="StrengthtabClick('@_strength.PidfproductStrengthId','@Model.Pidfpbfid', '@_strength.Strength');" id="Strengthtab_@_strength.PidfproductStrengthId">@_strength.Strength</a>
								</li>
								k = k + 1;
								str = @_strength.Strength;
							}
                        }
                    </ul>
                </div>
            </div>
            @*-----------------End- of-Strength---------------------*@
